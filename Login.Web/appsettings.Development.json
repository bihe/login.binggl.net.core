      "CookieDomain": "devel.binggl.net",
      "CookieName": "login_auth",
      "GoogleClientId": "",
      "GoogleClientSecret": ""
    }
  },
  "ConnectionStrings": {
    "LoginConnection": "Data Source=./_db/login.data.db"
  }
}
